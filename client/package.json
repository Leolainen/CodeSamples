{
  "name": "codesamples",
  "version": "1.0.0",
  "description": "A website for finding code examples on different problems.",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "next --port 3000",
    "build": "next build && next export",
    "storybook": "start-storybook --ci -p 9001 -c .storybook",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --cache",
    "lint:css": "stylelint '{components,main,pages}/**/*.scss' --cache",
    "prettier": "prettier --write '{components,main,pages}/**/*.{js, css}'",
    "prettier:check": "prettier --list-different '{components,main,pages}/**/*.{js, css}'",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "Leo",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "cssnano": "^4.1.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.8.0",
    "eslint-config-lydell": "^10.0.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-css-modules": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lodash-webpack-plugin": "^0.11.5",
    "postcss-calc": "^7.0.1",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^4.1.0",
    "postcss-scss": "^2.0.0",
    "postcss-simple-vars": "^5.0.1",
    "prettier": "^1.15.1",
    "react-test-renderer": "^16.6.3",
    "stylelint": "^9.7.1",
    "stylelint-config-recommended": "^2.1.0"
