    "lodash": "^4.17.11",
    "next": "^9.3.3",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-ace": "^6.4.0",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.1",
    "react-final-form": "^4.0.2",
    "react-icons": "^3.4.0",
